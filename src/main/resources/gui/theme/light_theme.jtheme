Light Theme
* {
    -theme-foreground: #222222;
    -theme-foreground-darker: derive(-theme-foreground, 20%);
    -theme-foreground-darker-2: derive(-theme-foreground, 40%);
    -theme-foreground-lighter: #000000;
    -theme-background: #f2f2f2;
    -theme-background-darker: #d9d9d9;
    -theme-background-darker-2: #e6e6e6;
    -theme-background-lighter: #f7f7f7;
    -theme-background-pressed: derive(-theme-background, -20%);
    -theme-background-lightest: #FFFFFF;
    -theme-header: #8faccc;
    -theme-menu-item-hover: #4D6EAF;
    -theme-element-selected: #5a80cc;
}

/*REMOVE FOCUS INDICATOR (BLUE BORDER)*/
.focus-indicator {
    -fx-border-color: transparent;
}

/*LABEL*/
.label {
    -fx-text-fill: -theme-foreground;
    -fx-font-family: "{FONT_GENERAL}";
    -fx-font-size: 14px;
}

/*TEXT AREA*/
.text-area {
    -fx-background-color: -theme-background-lightest;
    -fx-text-fill: -theme-foreground;
    -fx-font-family: "{FONT_GENERAL}";
}

.text-area .scroll-pane {
    -fx-background-color: -theme-background-lightest;
}

.text-area .content {
    -fx-background-color: -theme-background-lightest;
    -fx-background-radius: 0;
}

/*TEXT FIELD*/
.text-field {
    -fx-font-family: "{FONT_GENERAL}";
    -fx-background-color: -theme-background-darker;
    -fx-text-fill: -theme-foreground;
    -fx-background-radius: 1;
    -fx-border-color: transparent;
}

.invalid-text-field {
    -fx-font-family: "{FONT_GENERAL}";
    -fx-background-color: #842020;
    -fx-text-fill: -theme-foreground;
    -fx-background-radius: 1;
    -fx-border-color: transparent;
}

/*PANE*/
AnchorPane {
    -fx-background-color: -theme-background;
}

/*MUST ADD THIS CLASS TO ELEMENTS*/
.anchor-pane {
    -fx-background-color: -theme-background;
}

/*TAB*/
.tab-pane .tab-header-area {
    -fx-padding: 0;
}

.tab-pane .tab-header-area .tab-header-background {
    -fx-background-color: -theme-background;
}

.tab-pane .tab-down-button {
    -fx-background-color: -theme-background;
}

.tab-pane .tab-header-area .tab {
    -fx-background-radius: 0;
    -fx-background-color: -theme-background;
    -fx-padding: 2 4 0 4;
    -fx-border-width: 0 2 3 0;
    -fx-border-color: -theme-background;
}

.tab-pane .tab-header-area .tab:selected {
    -fx-border-color: -theme-background -theme-background -theme-background-pressed -theme-background;
}

.tab-pane .tab-header-area .tab:showing {
    -fx-border-color: -theme-background -theme-background -theme-background-pressed -theme-background;
}

.tab-pane .tab-header-area .tab .tab-label {
    -fx-text-fill: -theme-foreground;
    -fx-font-family: "{FONT_GENERAL}";
    -fx-font-size: 14px;
}

.tab-pane .tab-header-area .tab-close-button {
    -fx-background-color: -theme-foreground;
}

.tab-pane .headers-region {
    -fx-background-color: -theme-background;
}

/*split*/
.split-pane {
    -fx-padding: 0px;
    -fx-background-color: -theme-background;
}

.split-pane:horizontal .split-pane-divider {
    -fx-background-color: -theme-background-darker;
    -fx-padding: 0 2 0 0;
}

.split-pane:vertical .split-pane-divider {
    -fx-background-color: -theme-background-darker;
    -fx-padding: 0 1 0 0;
}

/*scroll pane*/
.scroll-pane {
    -fx-background-color: transparent;
}

.scroll-pane .viewport {
    -fx-background-color: -theme-background;
}

.scroll-pane .corner {
    -fx-background-color: -theme-background;
}

/*SCROLL BAR*/
.scroll-bar {
    -fx-background-color: -theme-background;
}

.scroll-bar:horizontal .increment-button,
.scroll-bar:horizontal .decrement-button {
    -fx-background-color: transparent;
    -fx-background-radius: 0.0em;
    -fx-padding: 0.0 0.0 10.0 0.0;

}

.scroll-bar:vertical .increment-button,
.scroll-bar:vertical .decrement-button {
    -fx-background-color: transparent;
    -fx-background-radius: 0.0em;
    -fx-padding: 0.0 10.0 0.0 0.0;

}

.scroll-bar .increment-arrow,
.scroll-bar .decrement-arrow {
    -fx-shape: " ";
    -fx-padding: 0.15em 0.0;
}

.scroll-bar:vertical .increment-arrow,
.scroll-bar:vertical .decrement-arrow {
    -fx-shape: " ";
    -fx-padding: 0.0 0.15em;
}


.scroll-bar:horizontal .thumb,
.scroll-bar:vertical .thumb {
    -fx-background-color: -theme-background-lighter;
    -fx-background-radius: 0;
}

.scroll-bar:horizontal .thumb:pressed,
.scroll-bar:horizontal .thumb:hover,
.scroll-bar:vertical .thumb:pressed,
.scroll-bar:vertical .thumb:hover {
    -fx-background-color: -theme-background-pressed;
    -fx-background-radius: 0;
}


.scroll-bar:horizontal .track,
.scroll-bar:vertical .track {
    -fx-background-color: transparent;
    -fx-border-color: transparent;
    -fx-background-radius: 0.0em;
    -fx-border-radius: 2.0em;
}


/*BUTTON*/
.button {
    -fx-background-color: -theme-background-darker;
    -fx-border-color: transparent;
    -fx-background-radius: 5;
    -fx-text-fill: -theme-foreground;
}

.button:hover {
    -fx-background-color: -theme-background-lighter;
}

.button:pressed {
    -fx-background-color: -theme-background-pressed;
}

.bold-button {
    -fx-background-color: -theme-background;
    -fx-border-color: transparent;
    -fx-background-radius: 0;
    -fx-text-fill: -theme-foreground;
    -fx-font-size: 14px;
    -fx-font-weight: bold;
}

.bold-button:hover {
    -fx-background-color: -theme-background-lighter;
}

.bold-button:pressed {
    -fx-background-color: -theme-background-pressed;
}

.menu-button {
    -fx-background-color: -theme-background;
    -fx-border-color: -theme-background-lighter;
    -fx-background-radius: 0;
    -fx-text-fill: -theme-foreground;
}

.menu-button:hover {
    -fx-background-color: -theme-background-lighter;
    -fx-border-color: -theme-background-lighter;
    -fx-background-radius: 0;
    -fx-text-fill: -theme-foreground;
}

.menu-button .text {
    -fx-font-size: 15;
}

.toggle-button {
    -fx-background-color: -theme-background;
    -fx-border-color: -theme-background-lighter;
    -fx-background-radius: 0;
    -fx-text-fill: -theme-foreground;
}

.toggle-button:hover {
    -fx-background-color: -theme-background-lighter;
    -fx-border-color: -theme-background-lighter;
    -fx-background-radius: 0;
    -fx-text-fill: -theme-foreground;
}

.toggle-button:selected {
    -fx-background-color: -theme-background-pressed;
    -fx-border-color: -theme-background-pressed;
    -fx-background-radius: 0;
    -fx-text-fill: -theme-foreground;
}

/*RADIO BUTTON*/
.radio-button {
    -fx-text-fill: -theme-foreground;
}

.radio-button .radio {
    -fx-background-color: -theme-background-darker;
}

.radio-button:selected .dot {
    -fx-background-color: -theme-foreground;
}

/*CHECK BOX*/
.check-box {
    -fx-text-fill: -theme-foreground;
}

.check-box .box {
    -fx-background-color: -theme-background-lightest;
    -fx-background-radius: 1;
}

.check-box:selected > .box > .mark {
    -fx-background-color: -theme-foreground;
}

/*PROGRESS BAR*/
.progress-bar .track {
    -fx-background-color: -theme-background-darker;
}

.progress-bar .bar {
    -fx-background-color: -theme-background-pressed;
}

/*CHOICE BOX */
.choice-box {
    -fx-background-color: -theme-background;
    -fx-border-color: -theme-background-lighter;
}

/*LIST VIEW*/
.list-view {
    -fx-background-color: -theme-background;
}

.list-view .list-cell:even {
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background-darker;
}

.list-view .list-cell:odd {
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background-darker-2;
}

.list-view .corner {
    -fx-background-color: -theme-background;
}

/* COMBO BOX */
.combo-box-base {
    -fx-background-color: -theme-background-lightest;
    -fx-border-color: -theme-background-darker;
    -fx-border-radius: 4 4 4 4;
    -fx-border-width: 2 2 2 2;
}

.combo-box-base .arrow-button {
    -fx-background-color: -theme-background-lightest;
}

.combo-box-base .list-cell {
    -fx-text-fill: -theme-foreground;
    -fx-font-size: 14px;
    -fx-background-color: -theme-background-lightest;
    -fx-padding: 1 1 1 3;
}

.combo-box-base .combo-box-popup .list-view {
    -fx-background-color: -theme-background-lightest;
    -fx-text-fill: -theme-foreground;
    -fx-font-size: 14px;
}

.combo-box-base .list-view .list-cell {
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background-lightest;
}

.combo-box-base .list-view .list-cell:hover {
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background-pressed;
}

/*TABLE VIEW*/
.table-view {
    -fx-background-color: -theme-background;
}

.table-view .corner {
    -fx-background-color: -theme-background;
}

.table-view .filler {
    -fx-background-color: -theme-background-lightest;
}

.table-view .column-header {
    -fx-background-color: -theme-background-lightest;
}

.table-view .table-row-cell:even {
    -fx-background-color: -theme-background-darker-2;
}

.table-view .table-row-cell:odd {
    -fx-background-color: -theme-background-darker;
}

.table-view .table-row-cell:selected {
    -fx-background-color: -theme-background-lighter;
}

.table-view .table-cell {
    -fx-text-fill: -theme-foreground;
}

.table-view .column-drag-header {
    -fx-background-color: -theme-background-darker;
}

.table-view .table-row-cell {
    -fx-table-cell-border-color: -theme-background;
}

.table-view .column-resize-line {
    -fx-background-color: -theme-foreground;
}

/*TREE TABLE VIEW*/
.tree-table-view {
    -fx-background-color: -theme-background;
}

.tree-table-view .filler {
    -fx-background-color: -theme-background-darker;
}

.tree-table-view .column-header {
    -fx-background-color: -theme-background-darker;
}

.tree-table-view .table-row-cell:even {
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background-lighter;
}

.tree-table-view .table-row-cell:odd {
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background-pressed;
}

.tree-table-view .column-drag-header {
    -fx-background-color: -theme-background-darker;
}

.tree-table-view .table-row-cell {
    -fx-table-cell-border-color: -theme-background;
}

.tree-table-view .column-resize-line {
    -fx-background-color: -theme-foreground;
}

/* Special class used to hide the horizontal scroll bar */
.table-view-horizontal-fit .scroll-bar:horizontal {
    -fx-opacity: 0;
    -fx-padding: -7;
}

/*TREE VIEW*/
.tree-view {
    -fx-background-color: -theme-background;
    -fx-text-fill: -theme-foreground;
}

/*PAGINATION*/
.pagination .pagination-control .number-button {
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background-darker;
}

/*PIE CHART*/
.chart-pie {
    -fx-border-color: transparent;
    -fx-border-width: 0;
}

/* Using -fx-background-color instead of -fx-pie-color. This removes the light effect */
.default-color0.chart-pie {
    -fx-background-color: #2f75cb;
}

.default-color1.chart-pie {
    -fx-background-color: #cb2f41;
}

.default-color2.chart-pie {
    -fx-background-color: #59cb2f;
}

.default-color3.chart-pie {
    -fx-background-color: #cbb62f;
}

.default-color4.chart-pie {
    -fx-background-color: #a12fcb;
}

.default-color5.chart-pie {
    -fx-background-color: #2fcb87;
}

.default-color6.chart-pie {
    -fx-background-color: #2f32cb;
}

.chart-pie-label {
    -fx-fill: -theme-foreground;
    -fx-font-size: 14;
}

.chart-legend {
    -fx-background-color: -theme-background-darker;
}

/*SEPARATOR*/
.separator {
    -fx-background-color: -theme-background-darker;
}

.separator .line {
    -fx-border-color: transparent;
}

/*MENU BAR*/
.menu-bar {
    -fx-background-color: -theme-background;
    -fx-padding: 0;
}

.menu {
    -fx-border-radius: 0;
    -fx-border-color: transparent;
    -fx-text-fill: -theme-foreground;
    -fx-background-color: -theme-background;
    -fx-padding: 5 5 5 5;
}

.menu .label {
    -fx-font-size: 13;
}

.menu:hover {
    -fx-background-color: -theme-background-lighter;
}

.menu:showing {
    -fx-background-color: -theme-background-lighter;
}

.menu-item {
    -fx-background-color: -theme-background;
    -fx-padding: 2 5 2 5;
}

.menu-item .label {
    -fx-font-size: 14;
}

.menu-item:hover,
.menu-item:selected {
    -fx-background-color: -theme-menu-item-hover;
}

.context-menu {
    -fx-background-color: -theme-background;
    -fx-padding: 0px;
}

.context-menu .separator {
    -fx-background-color: transparent;
}

.context-menu .separator .line {
    -fx-border-color: -theme-background-lighter transparent transparent transparent;
    -fx-border-insets: 1 0 0 0;
}

/*SLIDER*/
.slider * {
    -fx-background-color: -theme-background-darker;
}

.slider .thumb {
    -fx-background-color: -theme-background-lighter;
}

/* TOOLTIP */
.tooltip {
    -fx-background-color: -theme-background;
    -fx-border-color: -theme-background-lighter;
    -fx-border-width: 1;
    -fx-border-radius: 0;
    -fx-background-radius: 0;
}

/* WINDOW BUTTON*/
.window-button {
    -fx-border-color: transparent;
    -fx-border-width: 0;
    -fx-background-color: -theme-background;
    -fx-background-radius: 0;
}

.window-button:hover {
    -fx-background-color: -theme-background-lighÂºter;
}

.window-button:pressed {
    -fx-background-color: -theme-background-pressed;
}

.window-button-close {
    -fx-graphic: url("/gui/icon/window/close_black.png");
}

.window-button-minimize {
    -fx-graphic: url("/gui/icon/window/minimize_black.png");
}


.window-button-maximize {
    -fx-graphic: url("/gui/icon/window/maximize_black.png");
}


.window-button-unmaximize {
    -fx-graphic: url("/gui/icon/window/unmaximize_black.png");
}


/* START WINDOW */
.start-top-bar {
    -fx-background-color: -theme-background-darker-2;
}

.start-top-bar-window-button-minimize {
    -fx-background-color: -theme-background-darker-2;
}

.start-top-bar-window-button-close {
    -fx-background-color: -theme-background-darker-2;
}

.start-title {
    -fx-font-size: 30;
}

.start-subtitle {
    -fx-font-size: 20;
    -fx-text-fill: -theme-foreground-darker;
}

/* PROJECT TAB PANE */
.project-tab-pane {
    -fx-background-color: -theme-background-darker-2;
}

.project-tab-pane .tab-header-area {
    -fx-background-color: -theme-background-darker-2;

}

.project-tab-pane .tab-header-area .tab-header-background {
    -fx-background-color: -theme-background-darker-2;
}

.project-tab-anchor-pane {
    -fx-background-color: -theme-background-darker-2;
}

.project-tab-pane .tab-header-area .tab {
    -fx-background-color: -theme-background-darker-2;
    -fx-border-color: -theme-background-darker-2;
}

.project-tab-pane .tab-header-area .tab:selected {
    -fx-border-color: -theme-background-darker-2 -theme-background-darker-2 -theme-background-pressed -theme-background-darker-2;
}

.buttons-hbox {
    -fx-background-color: -theme-background-darker-2;
}

.buttons-hbox-button {
    -fx-padding: 0;
    -fx-background-color: -theme-background-darker-2;
    -fx-border-color: transparent;
    -fx-background-insets: 2;
    -fx-background-radius: 5;
    -fx-text-fill: -theme-background-darker-2;
    -fx-font-size: 14px;
    -fx-font-weight: bold;
}

/*ABOUT*/
.about-text {
    -fx-font-size: 18px;
    -fx-text-alignment: center;
}

/*SIDEBAR*/
.sidebar {
    -fx-background-color: -theme-background-darker-2;
}

.sidebar-left {
    -fx-border-width: 0 1 0 0;
}

.sidebar-right {
    -fx-border-width: 0 0 0 1;
}

/*SIDEBAR BUTTON*/
.sidebar-button {
    -fx-background-color: -theme-background-darker-2;
    -fx-border-width: 0;
    -fx-padding: 10 0 5 0;
}

.sidebar-button .label {
    -fx-font-size: 12px;
}

.sidebar-button:selected {
    -fx-background-color: -theme-background-darker;
}


.sidebar-button-right .label {
    -fx-rotate: 90;
}

.sidebar-button-left .label {
    -fx-rotate: -90;
}

/*SIDE PANE NODE HEADER*/
.side-pane-node-header {
    -fx-background-color: -theme-background;
}

.side-pane-node-header:hover,
.side-pane-node-header:pressed {
    -fx-background-color: -theme-header;
}

.side-pane-node-header-button {
    -fx-background-color: transparent;
    -fx-background-radius: 0;
}

/*PROJECT MENU*/
.project-menu {
    -fx-background-color: -theme-background-darker;
}

/*FOLDER EXPLORER*/
.explorer-folder .label,
.explorer-file .label {
    -fx-border-width: 1;
    -fx-border-color: transparent;
}

.explorer-folder-allow-drop > .label,
.explorer-file-allow-drop > .label {
    -fx-border-color: red;
}

/*BOTTOM BAR*/
.bottom-bar {
    -fx-background-color: -theme-background-darker-2;
}

/*BOTTOM BAR BUTTON*/
.bottom-bar-button {
    -fx-background-color: -theme-background-darker-2;
    -fx-border-width: 0;
    -fx-padding: 0 10 0 10;
}

.bottom-bar-button .label {
    -fx-font-size: 12px;
}

.bottom-bar-button:selected {
    -fx-background-color: -theme-background-darker;
}

/*BOTTOM PANE NODE HEADER*/
.bottom-pane-node-header {
    -fx-background-color: -theme-background;
}

.bottom-pane-node-header:hover,
.bottom-pane-node-header:pressed {
    -fx-background-color: -theme-header;
}

.explorer-section {
    -fx-spacing: 0;
}

.explorer-section .contents {
    -fx-spacing: 0;
}

.explorer-element {
    -fx-background-color: -theme-background;
    -fx-padding: 2 2 2 2;
}


.explorer-element .label {
    -fx-font-size: 14px;
}

.selected-explorer-element {
    -fx-background-color: -theme-element-selected;
}

.explorer-jams-folder {
    -fx-background-color: #f1c4c4;
}

.explorer-jams-folder.selected-explorer-element {
    -fx-background-color: -theme-element-selected;
}

/* CONFIG */
.configuration-window-explorer {
    -fx-background-color: -theme-background-darker-2;
}

.configuration-window-node {
    -fx-spacing: 5px;
}

.configuration-window-node .label {
    -fx-font-size: 14px;
}

.configuration-window-section .explorer-element {
    -fx-background-color: -theme-background-darker-2;
}

.configuration-window-section .selected-explorer-element {
    -fx-background-color: -theme-element-selected;
}

.configuration-window-display {
    -fx-spacing: 5px;
}

.configuration-window-display-contents {
    -fx-spacing: 7px;
}

.configuration-window-display-contents .label {
    -fx-padding: 0 0 0 5;
}

.configuration-section-tree-display {
    -fx-font-weight: bold;
    -fx-text-fill: -theme-foreground-darker-2;
    -fx-font-size: 12;
    -fx-padding: 10 0 0 15;
}

.configuration-window-region-label {
    -fx-text-fill: -theme-foreground-darker-2;
    -fx-alignment: top-left;
}

.configuration-window-region-separator {
    -fx-background-color: transparent;
    -fx-border-color: -theme-foreground-darker-2;
    -fx-border-width: 0 0 1 0;

    -fx-padding: 7 0 0 0;
}

/* ACTION CONFIG */
.action-remove-button {
    -fx-padding: 1 1 1 1;
}

.action-remove-button:hover {
    -fx-background-color: #ffb2b2;
}

.action-add-button {
    -fx-padding: 1 3 1 3;
}

/* FILE DISPLAY LIST */
.file-editor-list {
    -fx-background-color: -theme-background-lightest;
}

.file-editor-list .tab-header-area .tab-header-background {
    -fx-background-color: -theme-background;
}

.file-editor-list .tab-header-area .tab:selected {
    -fx-background-color: -theme-background-lighter;
    -fx-border-color: -theme-background-lighter -theme-background-lighter -theme-background-pressed -theme-background-lighter;
}

.draggable-file-editor-list:hover {
    -fx-background-color: -theme-header;
}

.draggable-file-editor-list .tab-header-area .tab-header-background:hover {
    -fx-background-color: -theme-header;
}

/* WINDOW POPUPS */
.window-popup-background {
    -fx-border-color: -theme-background-lighter;
    -fx-border-width: 1px;
}

/* CODE AREA */
.code-area {
    -fx-background-color: -theme-background-lightest;
    -fx-text-fill: -theme-foreground;
}

.code-area .scroll-pane {
    -fx-background-color: -theme-background-lightest;
}

.caret {
    -fx-stroke: -theme-foreground;
}

.paragraph-box:has-caret {
    -fx-background-color: -theme-background-darker;
}

/*Default text*/
.styled-text-area .text {
    -fx-fill: -theme-foreground;
    -fx-font-family: "{FONT_CODE}";
    -fx-font-size: 13.5;
}

.code-area .paragraph-text {
    -fx-padding: 0 0 0 5px;
}

.code-area .selection {
    -fx-fill: #a7d2ff;
}


/* NUMBER LINE */

.code-area .left-bar {
    -fx-border-width: 0 1 0 0;
    -fx-border-color: -theme-background-lighter;
    -fx-background-color: -theme-background-darker;
}

.code-area .left-bar-background {
    -fx-fill: -theme-background-darker;
}

.code-area .left-bar .line-number {
    -fx-background-color: transparent;
}

.code-area .left-bar .line-number .text {
    -fx-fill: -theme-background-lighter;
    -fx-font-family: "{FONT_CODE}";
    -fx-font-size: 12;
}

.paragraph-box .left-bar .line-number .text {
    -fx-fill: -theme-foreground-darker;
}

.paragraph-box:has-caret .left-bar .line-number .text {
    -fx-fill: -theme-foreground-lighter;
}

/* AUTOCOMPLETION */
.autocompletion-popup {
    -fx-background-color: -theme-background;
    -fx-border-color: -theme-background-darker-2;
    -fx-border-width: 1px;
}

.autocompletion-popup-element .label {
    -fx-padding: 0px 5px 0px 5px;
    -fx-font-family: "{FONT_CODE}";
    -fx-font-size: 13.5;
}

.autocompletion-popup-element-selected {
    -fx-background-color: -theme-element-selected;
}

/* LOG */
.code-area .log_error {
    -fx-fill: #800202;
}

.code-area .log_info {
    -fx-fill: #020280;
}

.code-area .log_warning {
    -fx-fill: #806f02;
}

.code-area .log_done {
    -fx-fill: #1d8002;
}

/* MIPS */
.code-area .mips-comment {
    -fx-fill: #757575;
}

.code-area .mips-label {
    -fx-font-style: italic;
}

.code-area .mips-global-label {
    -fx-fill: #662a22;
    -fx-font-style: italic;
}

.code-area .mips-directive {
    -fx-fill: #664227;
}

.code-area .mips-directive-parameter {
    -fx-fill: #66275e;
}

.code-area .mips-directive-parameter-string {
    -fx-fill: #4c762e;
}

.code-area .mips-instruction {
    -fx-fill: #be9f33;
}

.code-area .mips-instruction-parameter-label {
    -fx-fill: #000000;
    -fx-font-style: italic;
}

.code-area .mips-instruction-parameter-global-label {
    -fx-fill: #662a22;
    -fx-font-style: italic;
}

.code-area .mips-instruction-parameter-register {
    -fx-fill: #3a4166;
}

.code-area .mips-instruction-parameter-immediate {
    -fx-fill: #326627;
}


.code-area .mips-instruction-parameter-string {
    -fx-fill: #665827;
}

.code-area .mips-error {
    -rtfx-underline-color: #661717;
    -rtfx-underline-width: 1px;
}

/* MIPS POPUPS*/
.mips-popup {
    -fx-background-color: -theme-background-lighter;
    -fx-padding: 5px 5px 5px 5px;
    -fx-border-color: -theme-background-lighter;
    -fx-border-width: 1px;
}

/* MIPS EXPLORER */
.mips-explorer-file-to-assemble {
    -fx-background-color: #8eb381;
}

/*Added and selected*/
.mips-explorer-file-to-assemble.selected-explorer-element {
    -fx-background-color: #6f96b3;
}

/* MIPS CONFIGURATIONS */
.mips-configurations-window-name-field {
    -fx-alignment: center-left;
    -fx-spacing: 5;
    -fx-padding: 5 5 5 5;
}

/* ---------- */
/* SIMULATION */
/* ---------- */

.console .prompt {
    -fx-background-color: -theme-background-lightest;
    -fx-border-width: 1 0 0 0;
    -fx-border-color: -theme-background;
}

.console .inputs {
    -fx-background-color: -theme-background-lightest;
    -fx-border-width: 0 0 0 1;
    -fx-border-color: -theme-background;
}

.console .input {
    -fx-padding: 0 0 0 3;
    -fx-background-radius: 1;
}

.console .input-string .label {
    -fx-text-fill: #1f3314;
}

.console .input-integer .label {
    -fx-text-fill: #408033;
}

.console .input-float .label {
    -fx-text-fill: #305d80;
}

.console .input:hover {
    -fx-background-color: darkred;
}

/* INSTRUCTION FLOW */
.flow-entry-odd {
    -fx-background-color: -theme-background-darker-2;
}

.flow-entry-odd .flow-entry-instruction-label {
    -fx-background-color: -theme-background-darker-2;
}

.flow-entry-even {
    -fx-background-color: -theme-background-darker;
}

.flow-entry-even .flow-entry-instruction-label {
    -fx-background-color: -theme-background-darker;
}

.flow-entry-selected {
    -fx-background-color: -theme-background-lighter;
}

.flow-entry-selected .flow-entry-instruction-label {
    -fx-background-color: -theme-background-lighter;
}

.flow-current-cycle-label {
    -fx-background-color: -theme-background;
}

/* ----------------------- */
/* SINGLE CYCLE SIMULATION */
/* ----------------------- */

/* INSTRUCTION TABLE */
.table-view .single-cycle-next-instruction {
    -fx-background-color: #cccc7e;
}

.table-view .single-cycle-current-instruction {
    -fx-background-color: #a3bbcc;
}

/*FLOW*/
.flow-entry .single-cycle-execute {
    -fx-background-color: #a3bbcc;
}

/* ---------------------- */
/* MULTI CYCLE SIMULATION */
/* ---------------------- */

/* INSTRUCTION TABLE */
.table-view .multi-cycle-fetch {
    -fx-background-color: #cccc7e;
}

.table-view .multi-cycle-decode {
    -fx-background-color: #a3cc7c;
}

.table-view .multi-cycle-execute {
    -fx-background-color: #a3bbcc;
}

.table-view .multi-cycle-memory {
    -fx-background-color: #c0b6cc;
}

.table-view .multi-cycle-write-back {
    -fx-background-color: #ccafad;
}

/*FLOW*/
.flow-entry .multi-cycle-fetch {
    -fx-background-color: #cccc7e;
}

.flow-entry .multi-cycle-decode {
    -fx-background-color: #a3cc7c;
}

.flow-entry .multi-cycle-execute {
    -fx-background-color: #a3bbcc;
}

.flow-entry .multi-cycle-memory {
    -fx-background-color: #c0b6cc;
}

.flow-entry .multi-cycle-write-back {
    -fx-background-color: #ccafad;
}

/*CACHES*/
.cache-log-message .hit {
    -fx-text-fill: #4b6630;
}

.cache-log-message .miss {
    -fx-text-fill: #664645;
}

.cache-log-message .label {
    -fx-font-size: 13px;
}